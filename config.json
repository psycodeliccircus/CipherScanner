{
  "version": "11.0.0",
  "signatures": [
    "/help%w+Code/",
    "assert",
    "RegisterNetEvent",
    "PerformHttpRequest"
  ],
  "fileExts": ["lua", "js", "json"],
  "skipDirs": {
    "node_modules": true,
    "stream": true,
    "_cfx_internal": true
  },
  "skipFiles": [".test.lua", "deprecated.lua"],
  "maxOpsPerFrame": 30,
  "verbose": false,
  "incremental": true,
  "autoOnStart": true,
  "autoInterval": 3600000,
  "logLevel": "info",
  "showScanned": true,
  "update": {
    "enabled": true,
    "checkInterval": 86400000,
    "github": {
      "owner": "psycodeliccircus",
      "repo": "CipherScanner",
      "token": ""
    }
  },
  "webhook": {
    "enabled": false,
    "url": ""
  }
}
